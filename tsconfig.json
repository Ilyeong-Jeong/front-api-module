{
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "noImplicitAny": true,
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "strictPropertyInitialization": false,
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}